{
    "compilerOptions": {
      "module": "es6",
      "target": "es6",
      "noImplicitAny": true,
      "removeComments": true,
      "moduleResolution": "node",
      "preserveConstEnums": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "outDir": "dist",
    },
    "include": ["components/*", "models/*", "src/**/*", "tests/**/*", "utils/*", "config/*", "app.ts"],
  }

